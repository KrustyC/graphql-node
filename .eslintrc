{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "jest": true
    },
    "plugins": [
      "import",
      "jsx-a11y"
    ],
    "rules": {
      "comma-dangle": ["error", "never"],
      "semi": [ "error", "never" ],
      "no-underscore-dangle": "off",
      "class-methods-use-this": 0,
      "generator-star-spacing": "off",
      "global-require": 0,
      "import/no-extraneous-dependencies": 0
    }
  }